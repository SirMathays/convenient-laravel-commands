{
    "name": "sirmathays/convenient-laravel-commands",
    "description": "Adds some very convenient commands to your Laravel project",
    "keywords": [
        "laravel",
        "commands"
    ],
    "homepage": "https://matti.suoraniemi.com/creations/packages/convenient-laravel-commands",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": ">=6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "SirMathays\\Convenience\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SirMathays\\Convenience\\ExtraCommandServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matti Suoraniemi",
            "email": "matti@suoraniemi.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
