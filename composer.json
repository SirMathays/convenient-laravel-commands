{
    "name": "sirmathays/convenient-laravel-commands",
    "description": "Adds some very convenient commands to your Laravel project",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^8.25"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "SirMathays\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SirMathays\\ServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matti Suoraniemi",
            "email": "sir@mathays.xyz"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
